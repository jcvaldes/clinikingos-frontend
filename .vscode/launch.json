  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Iniciar server",
      "program": "${workspaceFolder}/server/app.js",
      "runtimeExecutable": "${workspaceFolder}/server/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--nolazy"
      ]
    },
    {

      "type": "node",
      "request": "launch",
      "name": "Iniciar home",
      "program": "${file}",
      "preLaunchTask": "tsc: build - home/tsconfig.json",
      "runtimeExecutable": "${workspaceFolder}/home/node_modules/.bin/ts-node",
      "runtimeArgs": [
        "--nolazy"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "console": "integratedTerminal" 
    }
  ]
}


